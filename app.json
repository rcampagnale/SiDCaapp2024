{
  "expo": {
    "name": "SiDCa - Tu Sindicato",
    "slug": "sidca",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logos/logo-05.png",
    "scheme": "sidca",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logos/logo-01.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.jakiro12.one",
      "versionCode": 14,
      "adaptiveIcon": {
        "foregroundImage": "./assets/apk.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["CAMERA"],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            { "scheme": "sidca", "host": "asistencia" }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jakiro12.one",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Se utiliza para escanear el c√≥digo QR de asistencia."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "4914455f-9037-4d80-b27c-ca5ac02ba57a"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "rcampagnale"
  }
}
