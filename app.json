{
  "expo": {
    "name": "SiDCa - Tu Sindicato",
    "slug": "sidca",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/logo-05.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logos/logo-05.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.jakiro12.one",
      "adaptiveIcon": {
        "foregroundImage": "./assets/apk.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jakiro12.one",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "4914455f-9037-4d80-b27c-ca5ac02ba57a"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "rcampagnale"
  }
}
